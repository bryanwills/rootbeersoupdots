#%Module

set-alias ..   "cd .."
set-alias ...  "cd ../.."
set-alias .... "cd ../../.."

set-alias b    "cd -"
set-alias d    "cd ~/dotfiles"
set-alias h    "cd ~"
set-alias p    "cd ~/projects"

set-alias l    "ls -F"
set-alias la   "ls -AF"
set-alias ll   "ls -AFlo"

set-alias t "type"
set-alias ta "type -a"
